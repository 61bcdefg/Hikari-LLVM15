# RUN: llc -mtriple=aarch64--- -run-pass=prologepilog -run-pass=machine-outliner -verify-machineinstrs %s -o - | FileCheck %s
# Ensure that we don't emit a PACIBSP if our outlined function doesn't end in
# a RETAB.
--- |
  define void @baz() #0 {
    ret void
  }

  define void @bar(i32 %a) #0 {
    ret void
  }

  attributes #0 = { noredzone }
...
---

name:            bar
tracksRegLiveness: true
body:             |
  bb.0:
    liveins: $w0, $lr, $w8
    $sp = frame-setup SUBXri $sp, 32, 0
    $fp = frame-setup ADDXri $sp, 16, 0

  bb.1:
    ; CHECK-LABEL: name:            bar
    ; CHECK: BL @OUTLINED_FUNCTION
    BL @baz, implicit-def dead $lr, implicit $sp
    $w17 = ORRWri $wzr, 1
    BL @baz, implicit-def dead $lr, implicit $sp
    $w17 = ORRWri $wzr, 4
    BL @baz, implicit-def dead $lr, implicit $sp
    $w0 = ORRWri $wzr, 4

    ; CHECK-DAG: BL @OUTLINED_FUNCTION
    BL @baz, implicit-def dead $lr, implicit $sp
    $w17 = ORRWri $wzr, 1
    BL @baz, implicit-def dead $lr, implicit $sp
    $w17 = ORRWri $wzr, 4
    BL @baz, implicit-def dead $lr, implicit $sp
    $w0 = ORRWri $wzr, 3

    ; CHECK-DAG: BL @OUTLINED_FUNCTION
    BL @baz, implicit-def dead $lr, implicit $sp
    $w17 = ORRWri $wzr, 1
    BL @baz, implicit-def dead $lr, implicit $sp
    $w17 = ORRWri $wzr, 4
    BL @baz, implicit-def dead $lr, implicit $sp
    $w0 = ORRWri $wzr, 2

    ; CHECK-DAG: BL @OUTLINED_FUNCTION
    BL @baz, implicit-def dead $lr, implicit $sp
    $w17 = ORRWri $wzr, 1
    BL @baz, implicit-def dead $lr, implicit $sp
    $w17 = ORRWri $wzr, 4
    BL @baz, implicit-def dead $lr, implicit $sp
    $w0 = ORRWri $wzr, 1
  bb.2:
    $fp, $lr = LDPXi $sp, 2
    RET undef $lr
...
---
name:            baz
tracksRegLiveness: true
body:             |
  bb.0:
    liveins: $w0, $lr, $w8
    RET undef $lr

# CHECK-LABEL: name:            OUTLINED_FUNCTION
# CHECK-NOT: PACIBSP
# CHECK: TCRETURNdi

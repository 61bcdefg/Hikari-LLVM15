// Test that we don't crash when SILGen(ing) property wrappers
// [public].
// REQUIRES: swift

// RUN: %lldb --repl < %s

@propertyWrapper
struct Wrapper<T> {
  var wrappedValue: T

  init(initialValue: T) {
    wrappedValue = initialValue
  }
}

struct Foo {
  var x: Int = 17
  @Wrapper var y: Int = 42
}

func patatino() {
  _ = Foo(x: 5)
}

patatino()
